<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alaythiaproductions.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Emarosa"
            android:label="@string/category_emarosa"></activity>
        <activity android:name=".ForeverCameCalling"
            android:label="@string/category_forever_came_calling"/>
        <activity android:name=".KnucklePuck"
            android:label="@string/category_knuckle_puck"/>
        <activity android:name=".DanceGavinDance"
            android:label="@string/category_dance_gavin_dance"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>